<script type="text/javascript">
    require(["jquery","Lilysilk_TBase/js/model"], function($) {
        var country = $(".minicart-wrapper").attr("data-store");
        var currency = "";
        if($("#currencyName").text() == "") {
            currency = "";
        }else {
            currency = "/"+$("#currencyName").text();
        }
        var countrySelect = country+currency;
        $(".country-select-block").find(".country-select").text(countrySelect);
    });
</script>
