<script>
    //读取cookie的方法
    function getCookie(cname)
    {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++)
        {
            var c = ca[i].trim();
            if (c.indexOf(name)==0) return c.substring(name.length,c.length);
        }
        return "";
    }
    function abtest(){
        var lily_ab = null;
        try{
            var name = "Testeven" + "=";
            var ca = document.cookie.split(';');
            for(var i=0; i<ca.length; i++)
            {
                var c = ca[i].trim();
                if (c.indexOf(name)==0)
                {
                    lily_ab = "B";
                }

            }
        }catch(e) {}
        try{
            var name = "TestOdd" + "=";
            var ca = document.cookie.split(';');
            for(var i=0; i<ca.length; i++)
            {
                var c = ca[i].trim();
                if (c.indexOf(name)==0)
                {
                    lily_ab = "A";
                }

            }
        }catch(e) {}

        return lily_ab;
    }
    var LoginStatus = '',user_id = '';
    try {
        if (!window.localStorage) {
            LoginStatus = "";
            user_id = "";
        } else {
            var storageData = '';
            if ((storageData = JSON.parse(localStorage.getItem("mage-cache-storage"))) && storageData.customer) {
                if (storageData['customer'] && storageData['customer']['_talkable_data']) {
                    LoginStatus = 1;
                    user_id = '';
                    if (storageData['customer']) {
                        user_id = storageData['customer']['user_id']
                    }
                }
            } else {
                LoginStatus = "";
                user_id = "";
            }
        }
    }catch (e) {
        LoginStatus = "";
        user_id = "";
        //无痕模式
        console.log(e);
    }
    var PageInfo = window.PageInfo;
    window.dataLayer = window.dataLayer || [];
    window.dataLayer = [{
        'webPageGrouping':PageInfo.config.webPageGroup || "",
        'ecomm_prodid':PageInfo.config.ecommProdid || "",
        'LoginStatus' : LoginStatus,//当前登录状态
        "PageLocation" : "https://www.lilysilk.com/au/bedding/sheets/fitted-sheets.html?product_list_order=bestselling&___from_store=au",
        "UserAgent" : "Mozilla/5.0 (Linux; Android 12; Pixel 5) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/96.0.4664.104 Mobile Safari/537.36",
        "Country" : "au",//当前站点
        "PageGroup1" : "",
        "PageGroup2" : "",
        "PageGroup3" : PageInfo.config.webPageGroup || "",
        "abtest" : abtest(),
        "cookie_id" : getCookie('MONO_CUSTOMER_COOKIE'),//cookie mono
        "re_order" : "",
        "country_alert_abtest" : abtest(),
        "now_catalog" : "",//当前分类id或者分类名只有分类页有
        "cid" : getCookie('_ga'),//cookie _ga
        "user_id" : user_id,//用户userid
        "pcat" :"Silk Fitted Sheets"//当前pcat,
    }];
    // console.log(window.dataLayer);
</script>


<script>
    var UrlRequest = new Object();
    UrlRequest = GetRequest();
    function GetRequest() {
        var url = location.search;
        var theRequest = new Object();

        if (url.indexOf("?") != -1) {
            var str = url.substr(1);
            strs = str.split("&");
            for(var i = 0; i < strs.length; i ++) {
                theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
            }
        }
        return theRequest;
    }
    var RequestArr = Object.keys(UrlRequest);
    var URLSource = '';
    if(RequestArr.length>0)
    {
        if("source" in UrlRequest && UrlRequest.source == 'shareasale')
        {
            URLSource = "shs";
        }
        else if("utm_source" in UrlRequest)
        {
            URLSource = UrlRequest.utm_source;
        }
        else if("src" in UrlRequest)
        {
            URLSource = UrlRequest.src;
        }
        else if("campaignid" in UrlRequest || "gclid" in UrlRequest)
        {
            URLSource = "google";
        }
        else if("fbclid" in UrlRequest)
        {
            URLSource = "fb";
        }
        else
        {
            if(document.referrer != '' && document.referrer.indexOf("lilysilk.com")== -1 && document.referrer.indexOf("lilysilk.se")== -1 && document.referrer.indexOf("lilysilk.jp")== -1)
            {
                URLSource = "uppath";
            }
        }
    }
    else
    {
        if(document.referrer != '' && document.referrer.indexOf("lilysilk.com")== -1 && document.referrer.indexOf("lilysilk.se")== -1 && document.referrer.indexOf("lilysilk.jp")== -1 && document.referrer.indexOf("paypal.com")== -1 && document.referrer.indexOf("afterpay.com")== -1 && document.referrer.indexOf("clearpay.com")== -1 && document.referrer.indexOf("adyen.com")== -1)
        {
            URLSource = "uppath";
        }
    }
    if(URLSource!='')
    {
        // console.log(URLSource)
        urlSourceSetCookie('LIAN_MENG_COOKIE',URLSource)
    }
    function urlSourceSetCookie(name,value){
        var Days = 60;
        var exp = new Date();
        exp.setTime(exp.getTime() + Days*24*60*60*1000);
        document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString() + "; path=/";
    }
</script>
