    <script type="text/javascript">
    require(["jquery","Lilysilk_AnalysisMonitor/js/lilysilk-ga","mage/translate"], function($,lilysilkGa,$t) {
        var errorMessage = "";
        var email_input = 0 //当前未触发邮箱输入事件
        var eventCategory = 'undefined';
        if($.cookie('SubscriptionMethod')){
            eventCategory = $.cookie('SubscriptionMethod');
        }
        $(".subscribe-input").mousedown( function() { // 点击恢复
            $(".subscribe-input").removeClass("error-inputStyle");
            $(".subscribe-error-messsage").fadeOut(300);
            errorMessage = "";
        });
        function errorOn() {  // 错误提示
            $(".subscribe-input").addClass("error-inputStyle");
            $(".subscribe-error-messsage").css("display","flex");
            $(".subscribe-error").html(errorMessage);
        };
        //点击订阅弹出窗的x-关闭弹窗
        $(document).one("click",".subscribe-popup .subscribe-popup-close.popup-subpopup-init",function(){
            $(document).trigger("gtm:CommonPush",{
                'event': '',
                'eventCategory': eventCategory,
                'eventAction': 'Click',
                'eventLabel': 'x',
                'eventValue': '',
                'eventNoninteraction': true
            });
        });
        //点击No,Thanks按钮-关闭弹窗
        $(document).on("click",".subscribeModel .no-thanks",function(){
            $(document).trigger("gtm:CommonPush",{
                'event': '',
                'eventCategory': eventCategory,
                'eventAction': 'Click',
                'eventLabel': 'No thanks',
                'eventValue': '',
                'eventNoninteraction': false
            });
        });
        $("#isGetCoupon").click(function () {
            //订阅提交的点击事件
            $(document).trigger("gtm:SubscriptionSubmit",{
                'event': '',
                'eventCategory': eventCategory,
                'eventAction': 'Subscription Submit',
                'eventLabel': 'CLAIM MY 10% OFF',
                'eventValue': '',
                'eventNoninteraction': false
            });
            //  前端表单验证
            var inputVal = $(".subscribe-input").val();
            var inputReg = /^([a-zA-Z0-9]+[_|\_|\.|\-]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.|\-]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
            if( inputVal == "") {
                errorMessage = $t("This is a required field.");
                errorOn();
            }
            if(!inputReg.test(inputVal)){
                errorMessage = $t("Please enter a valid email address.");
                errorOn();
            }
            if( inputVal == "" || !inputReg.test(inputVal)) {
                return false;
            }
            var data = $("#subpopup-form-ajax").serializeArray();
            var formdata = {};
            $.each(data,function (index, value) {
                formdata[value.name] = value.value;
            });
            var form = new FormData();
            form.append("email", formdata['email']);
            form.append("lists", "");
            form.append("subscription_type", "subpopup_subscription");
            $.ajax({
                type: "post",
                url: "https://www.lilysilk.com/au/rest/V1/subscripution/subscribe_email/subEmail",
                dataType: "json",
                processData: false,
                mimeType: "multipart/form-data",
                contentType: false,
                data: form,
                success: function(data){
                    data = JSON.parse(data);//json字符转json对象
                    if(data.code==0){
                        console.log('成功');
                        //如果是第一次订阅成功的话写入手机订阅初始化值
                        if(!$.cookie('IS_SUBSCRIB')){
                            $.cookie('SMS_INIT', true);
                            console.log($.cookie('SMS_INIT'));
                        }
                                                $.cookie('IS_SUBSCRIB', true, {expires: 1095, path: '/' });//Set the cookies
                        //ga订阅成功回调
                        // lilysilkGa = new lilysilkGa();
                        // lilysilkGa.bindSubscriptionSuccess('popups_subscribe_input');
                        //新部署的订阅触发自定义事件
                        $(document).trigger("gtm:SubscribeSuccess",{
                            'event': 'SubscribeSuccess',
                            'eventCategory': eventCategory,
                            'eventAction': 'Subscribe Success',
                            'eventLabel': 'Popover_Subscription',
                            'eventValue': '',
                            'eventNoninteraction': false
                        });
                        var jsondata ={'email':formdata['email']}
                                                if(data.is_account){
                            jsondata['is_account'] = data.is_account
                        }else{
                            jsondata['is_account'] = data.is_account
                        }
                        $.ajax({
                            type: "post",
                            url: "https://www.lilysilk.com/au/subscripution/popup/guide",
                            data:jsondata,
                            success: function(data){
                                if(data){

                                    //关闭订阅弹窗
                                    $(".subscribe-popup").html(data);
                                    $(".subscribe-popup").find("p[role='button']").attr("tabindex","0");
                                    $(".subscribe-popup").find("div[role='button']").attr("tabindex","0");
                                    $(".subscribe-popup").find("button").attr("tabindex","0");
                                    $(".subscribe-popup").find("a").attr("tabindex","0");
                                }
                            }
                        })
                    }else {
                        var eventLabel = '';
                        //订阅失败后将原因写入
                        if(data.error_code == 5011) {  //  重复订阅
                            errorMessage = $t("This email address has already been subscribed.");
                            errorOn();
                            eventLabel = data.error_code+' This email address has already been subscribed.';
                        }else {
                            console.log(data.error_code);
                            console.log(data.msg);
                            eventLabel = data.error_code+' '+data.msg;
                        }
                        //新部署的订阅触发订阅失败事件
                        $(document).trigger("gtm:SubscribeError",{
                            'event': '',
                            'eventCategory': eventCategory,
                            'eventAction': 'Subscribe Error',
                            'eventLabel': eventLabel,
                            'eventValue': '',
                            'eventNoninteraction': false
                        });
                    }
                }
            })
        });

        //邮箱输入失去焦点事件-输入邮箱
        $("#subpopup-form-ajax .subscribe-input").on("blur",function () {
            var inputVal = $(".subscribe-input").val();
            //新部署的订阅 邮箱输入事件，此事件不能重复提交
            if(!email_input) {
                //如果未初始化调用，初始化后就不调用了
                $(document).trigger("gtm:SubscriptionEmailEnter", {
                    'event': '',
                    'eventCategory': eventCategory,
                    'eventAction': 'Email Input',
                    'eventLabel': inputVal,
                    'eventValue': '',
                    'eventNoninteraction': false
                });
            }
            email_input = 1;
        })
    })
</script></div>
