    require(["jquery","Lilysilk_TBase/js/abTest","mage/cookies"], function($,abTest) {
        //ab测试设置参数
        if (!$.cookie('AB_test')) {
            var timestamp = new Date().getTime()
            if (timestamp % 2) {
                $.cookie('AB_test', 'a', {expires: 1095, path: '/'});//Set the cookies
            } else {
                $.cookie('AB_test', 'b', {expires: 1095, path: '/'});//Set the cookies
