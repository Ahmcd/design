<script>
    require([
        'jquery',
        'slick',
    ], function($) {
        let dot = $("#search_sider .slick-dots")
        if (dot.length != 1) {
            $(".search_sider").slick({
                dots: true,
                infinite: true,
                arrows: false,
                speed: 1000,
                slidesToShow: 2,
                slidesToScroll: 2,
                fade: false,
                autoplay: true,
                autoplaySpeed: 2000,
                slide: "div",
            });
        }
        var alreadyLoadSearch = false;
        var Url = 'https://www.lilysilk.com/au/' + 'search';
        var search = $("#search");
        var downmenu = $(".search-downmenu");
        $(".search-block").click(function() {
            if(!alreadyLoadSearch)
            {
                alreadyLoadSearch = true;
                $.ajax({
                    url: Url,
                    type: "get",
                    dataType: "json",
                    success: function(data) {
                        var html = '';
                        var lastHtml = '';
                        var downmenuData = data;
                        var n = 0;
                        setInterval(showAuto, 5000);
                        for (var i = 0; i < data.length; i++) {
                            html = '<div><a href="' + data[i].url + '">';
                            center = '<span>' + (i + Number(1)) + '</span>'
                            lastHtml = '</a></div>';
                            document.getElementById("downmenu").innerHTML += (html + data[i].title + center + lastHtml);
                            $("#search").attr('placeholder', data[0].title);

                        };

                        function showAuto() {
                            for (var i = 0; i < data.length; i++) {};
                            if (n < downmenuData.length) {
                                n = Number(n) + 1;
                            } else if (n == downmenuData.length) {
                                n = 0;
                            };
                            let title = downmenuData[n].title;
                            $("#search").attr('placeholder', title);

                        };
                    },
                    error: function() {
                        console.log("error");
                    }
                });
            }


            $("#search_mini_form").submit(function(e) {
                var searchVal = search.val();
                var searchPla = search.attr('placeholder')
                if (searchVal == "") {
                    search.val(searchPla);
                    $("#search_mini_form").submit();
                }
            });
        });
    });
</script>
<script>require([
    'jquery',
    'mage/mage'
], function($){

    var dataForm = $('#form-validate');
    var ignore = 'input[id$="full"]';

    dataForm.mage('validation', {        errorPlacement: function(error, element) {
            if (element.prop('id').search('full') !== -1) {
                var dobElement = $(element).parents('.customer-dob'),
                    errorClass = error.prop('class');
                error.insertAfter(element.parent());
                dobElement.find('.validate-custom').addClass(errorClass)
                    .after('<div class="' + errorClass + '"></div>');
            }
            else {
                error.insertAfter(element);
            }
        },
        ignore: ':hidden:not(' + ignore + ')'    }).find('input:text').attr('autocomplete', 'off');
    dataForm.submit(function () {
        $(this).find(':submit').attr('disabled', 'disabled');
    });
    dataForm.bind("invalid-form.validate", function () {
        $(this).find(':submit').prop('disabled', false);
    });

});</script>


